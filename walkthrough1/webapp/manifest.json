{
  "_version": "1.12.0",
  "sap.app": {
	"id": "walkthrough1",
	"type": "application",
	"i18n": "i18n/i18n.properties",
	"title": "{{appTitle}}",
	"description": "{{appDescription}}",
	"applicationVersion": {
	  "version": "1.0.0"
	},
	"dataSources": {
		"invoiceRemote": {
			"uri": "https://services.odata.org/V2/Northwind/Northwind.svc/",
			"type": "OData",
			"settings": {
				"odataVersion": "2.0"
			}
		}
	}
  },
  "sap.ui": {
	"technology": "UI5",
	"deviceTypes": {
		"desktop": true,
		"tablet": true,
		"phone": true
	}
  },
  "sap.ui5": {
	"rootView": {
		"viewName": "walkthrough1.view.App",
		"type": "XML",
		"id": "app"
	},
	"dependencies": {
	  "minUI5Version": "1.93",
	  "libs": {
		"sap.ui.core": {},
		"sap.m": {}
	  }
	},
	"models": {
	  "i18n": {
		"type": "sap.ui.model.resource.ResourceModel",
		"settings": {
		  "bundleName": "walkthrough1.i18n.i18n",
		  "supportedLocales": [""],
		  "fallbackLocale": ""
		}
	  },
	  "invoice": {
		"dataSource": "invoiceRemote"
	  }
	},
	"resources": {
	  "css": [
		{
		  "uri": "css/style.css"
		}
	  ]
	},
	"routing": {
		"config": {
		  "routerClass": "sap.m.routing.Router",
		  "viewType": "XML",
		  "viewPath": "walkthrough1.view",
		  "controlId": "app",
		  "controlAggregation": "pages"
		},
		"routes": [
		  {
			"pattern": "",
			"name": "overview",
			"target": "overview"
		  },
		  {
			"pattern": "detail/{invoicePath}",
			"name": "detail",
			"target": "detail"
		  }
		],
		"targets": {
		  "overview": {
			"viewId": "overview",
			"viewName": "Overview"
		  },
		  "detail": {
			"viewId": "detail",
			"viewName": "Detail"
		  }
		}
	  }
  }
}